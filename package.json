    "@types/prompts": "^2.4.9",
    "@types/string-similarity": "^4.0.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vite": "^7.1.6",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "minimist": "^1.2.8",
    "prompts": "^2.4.2",
