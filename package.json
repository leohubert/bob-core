{
  "name": "bob-core",
  "version": "1.4.0",
  "description": "BOB Core",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "typesVersions": {
