        "./dist/cjs/*.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "node -r @swc-node/register debug/main.ts",
    "build": "rimraf ./dist && vite build",
    "prepare": "npm run build",
    "test": "vitest run"
  },
