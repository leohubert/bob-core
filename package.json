    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.14.5",
    "@types/prompts": "^2.4.9",
    "@types/string-similarity": "^4.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
